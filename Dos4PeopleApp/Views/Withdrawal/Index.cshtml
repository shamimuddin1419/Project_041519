
<div class="content-body">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">
                        Withdrawal Information Entry.
                        (
                        Main Balance : <b><label id="lblMinBalance" style="color:brown"></label></b>

                        Commission : <b><label id="lblCommission" style="color:brown"></label></b>
                        )
                         ~~Service Charge For(
                        Main Balance : <b><label id="lblMinBalanceSerCharge" style="color:brown"></label></b>%

                        Commission : <b><label id="lblCommissionSerCharge" style="color:brown"></label></b>%
                        )
                    </h4>
                </div>
                <div class="card-content" style="padding:0px;">
                    <div class="card-body" style="padding-top:0px;">
                        <div class="row">
                        <div class="col-md-4">
                            <div class="row">
                                <div class="col-xl-6 col-lg-3 col-md-3 mb-1">                                    
                                        <input type="checkbox" id="isMainBalance" @*class="switchery switchery-small"*@ />
                                        <label for="isMainBalance" class="font-medium-2 text-bold-600 ml-1">Main Balance</label>&nbsp;
                                    
                                </div>
                                <div class="col-xl-6 col-lg-6 col-md-3 mb-1">                                   
                                        <input type="checkbox" id="isCommission" @*class="switchery switchery-small"*@ />
                                        <label for="isCommission" class="font-medium-2 text-bold-600 ml-1"> Commission</label>&nbsp;
                                   
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">                                  
                                        <label for="basicInput"> Receive Payment Method</label>
                                        <select class="form-control select2" id="ddlReceivePaymentMethod">
                                            <option value="0" min="0">Select</option>
                                        </select>                                   
                                </div>
                                @*<div class="col-xl-1 col-lg-3 col-md-12 mb-1" style="padding:0px;">
                                    <img src="payImage.jpg" id="IdPMImage" width="60" height="50" />
                                </div>*@
                            </div>

                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">                                    
                                        <label for="basicInput"> Wallet Address</label>
                                        <input type="text" class="form-control" id="txtgWalletAddress">                                  
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">                                   
                                    <label for="basicInput"> Withdraw Amount</label> <label id="lblServiceCharge" style="color:brown"></label>
                                        <input type="number" onkeypress="return isNumberKey(this, event);" min="0" class="form-control allow_decimal" id="txtWIthdrawAmount">
                                       
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">                                   
                                        <label for="basicInput"> Password</label>
                                        <input type="password" class="form-control" id="txtPassword">                                    
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">                                   
                                        <label for="basicInput"> Remark</label>
                                        <textarea class="form-control" id="txtRemark" rows="2"></textarea>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xl-12 col-lg-4 col-md-12 mb-1">
                                    <button type="button" class="form-control-lg btn btn-primary btn-lg pl-3 pr-3" id="btnSubmitWithdrawalInfo">Submit Withdrawal Information</button>
                                </div>

                            </div>
                        </div>
                        <div class="col-md-8">
                            <div class="card">
                                <div class="card-content">
                                    <div class="card-body">
                                        <table id="ListTableId" class="table table-striped table-bordered" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Date </th>
                                                    <th>Type </th>
                                                    <th>Pmt. Method</th>
                                                    <th>Wallet Address</th>
                                                    <th>Amount</th>
                                                    <th>Status</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>
    </div>

@section scripts {
    <script src="~/Application_Scripts/PendingWithdraw.js"></script>
}

