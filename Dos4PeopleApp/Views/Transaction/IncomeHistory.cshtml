<div class="content-wrapper">
    <div class="content-header row">
        <div class="content-header-left col-md-6 col-12 mb-2">
            <h3 class="content-header-title mb-0">Income History</h3>
        </div>
    </div>
    <div class="content-body">
        <input type="hidden" id="searchUrlId" value="@Url.Action("GetIncomeHist","Transaction")" />
        <!-- Basic Elements start -->
        <section class="basic-elements">
            <div id="searchBox">
                <div class="card">
                    <div class="card-content">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-xl-4 col-lg-6 col-md-12 ">
                                    <div class="form-group">
                                        <label>From Date</label>
                                        <div class='input-group date' id='fromDatePickerId'>
                                            <input type="text" class="form-control" id="fromDateId" onkeydown="return false" />
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <span class="fa fa-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xl-4 col-lg-6 col-md-12 ">
                                    <div class="form-group">
                                        <label>To Date</label>
                                        <div class='input-group date' id='toDatePickerId'>
                                            <input type="text" class="form-control" id="toDateId" onkeydown="return false" />
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <span class="fa fa-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xl-4 col-lg-6 col-md-12" style="margin-top:2rem;">
                                    <div class="form-group">

                                        <button class="btn btn-info " id="btnSearchId">
                                            <i class="fa fa-search"></i>
                                            Search
                                        </button>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="card">
                    <div class="card-content">
                        <div class="card-body">
                            <table id="ListTableId" class="table table-striped table-bordered dataTable" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Amount($)</th>
                                        <th>Remarks</th>
                                        <th>Joined At</th>
                                        <th>Current Duration</th>
                                        <th>Expire At</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>
<script src="~/Content/Theme/app-assets/vendors/js/pickers/dateTime/moment-with-locales.min.js"></script>
<script src="~/Content/Theme/app-assets/vendors/js/pickers/dateTime/bootstrap-datetimepicker.min.js"></script>
<script src="~/Application_Scripts/IncomeHistory.js"></script>